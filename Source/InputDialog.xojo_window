#tag WindowCode
	#tag Method, Flags = &h0
		Shared Function ShowPrompt(promptTitle as String, promptText as String = "", DefaultAnswer as string = "") As String
		  Var win As New InputDialog
		  win.lblTitle.Text = promptTitle
		  win.lblDescription.Text = promptText
		  win.txtInput.Text = DefaultAnswer
		  
		  win.ShowModal
		  
		  Return win.PromptText
		End Function
	#tag EndMethod


	#tag Property, Flags = &h0
		PromptText As String
	#tag EndProperty


#tag EndWindowCode

#tag Events txtInput
	#tag Event
		Sub TextChange()
		  If Trim( Me.Text ) = "" Then
		    btnOkay.Enabled = False
		  Else
		    btnOkay.Enabled = True
		  End If
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events btnOkay
	#tag Event
		Sub Action()
		  PromptText = txtInput.Text
		  
		  Self.Close
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events btnCancel
	#tag Event
		Sub Action()
		  Self.Close
		End Sub
	#tag EndEvent
#tag EndEvents
